[sub_resource type="GDScript" id="GDScript_qxl0n"]
script/source = "extends Node3D

@onready var player := $Gameplay/StatesPlayer as PlayerController
@onready var controller_scheme_label := %ControllerSchemeLabel as Label
@onready var kb_mouse_scheme_label := %KbMouseSchemeLabel as Label
@onready var state_name_display := %StateNameDisplay as Label


func _ready() -> void:
	update_input_type(player.input_source.input_type)


func _process(_delta: float) -> void:
	update_state_name()


func update_input_type(input_type: int) -> void:
	kb_mouse_scheme_label.visible = false
	controller_scheme_label.visible = false
	
	if input_type == InputSource.InputType.Controller:
		controller_scheme_label.visible = true
	else:
		kb_mouse_scheme_label.visible = true


func update_state_name() -> void:
	state_name_display.text = \"(%s)\" % player.current_state_name


## Quit the game
func _unhandled_input(event: InputEvent) -> void:
	var is_esc: bool = (
			event is InputEventKey
			and event.keycode == Key.KEY_ESCAPE
	)
	var is_start: bool = (
			not is_esc
			and event is InputEventJoypadButton 
			and event.button_index == JoyButton.JOY_BUTTON_START
	)
	
	if is_esc or is_start:
		get_tree().quit()
"

